{
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "prettier.eslintIntegration": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  },
  "eslint.enable": true,
  "jest.autoEnable": false
}
