{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "jest.autoEnable": false,
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.next": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "css.validate": false,
  "stylelint.enable": true
}
